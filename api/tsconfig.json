{
	"compilerOptions": {
		"module": "commonjs",
		"outDir": "dist",
		"sourceMap": true,
		"strict": true,
		"target": "ES2015",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"alwaysStrict": true
	},
	"compileOnSave": true,
	"include": ["src"],
	"exclude": ["node_modules"]
}

